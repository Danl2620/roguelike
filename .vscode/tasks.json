{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Rust: cargo build - roguelike"
		},
		{
			"type": "cargo",
			"subcommand": "run",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Rust: cargo run - roguelike",
			"options": {
				"env": {"RUST_BACKTRACE": "1"}
			}
		}
	]
}